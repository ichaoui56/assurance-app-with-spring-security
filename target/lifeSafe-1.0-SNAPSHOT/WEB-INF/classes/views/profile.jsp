    <section class="profile-info-container">
        <!-- Profile Image -->
        <img src="https://w7.pngwing.com/pngs/81/570/png-transparent-profile-logo-computer-icons-user-user-blue-heroes-logo-thumbnail.png" alt="Profile Image" class="profile-image">

        <!-- User Name -->
        <h3 class="user-name">${user.name}</h3>

        <div class="user-info">
            <p><i class="fas fa-map-marker-alt"></i> <span>${user.address}</span></p>
            <p><i class="fas fa-envelope"></i> <span>${user.email}</span></p>
            <p><i class="fas fa-phone"></i> <span>${user.phone}</span></p>
        </div>

        <!-- User Contracts -->
        <div class="contracts-container">
            <h4>User Contracts</h4>
            <c:forEach var="contract" items="${contracts}">
                <div class="contract">
                    <p><strong>Insurance Type:</strong> ${contract.devis.insurance.type}</p>
                    <c:if test="${contract.devis.insurance.type == 'Automobile'}">
                        <p><strong>Driver Age:</strong> ${contract.devis.insurance.driverAge}</p>
                        <p><strong>Car Model:</strong> ${contract.devis.insurance.car.model}</p>
                    </c:if>

                    <c:if test="${contract.devis.insurance.type == 'Health'}">
                        <p><strong>Age:</strong> ${contract.devis.insurance.age}</p>
                        <p><strong>Chronic Illness:</strong> ${contract.devis.insurance.chronicIllness}</p>
                    </c:if>

                    <c:if test="${contract.devis.insurance.type == 'Housing'}">
                        <p><strong>Home Value:</strong> ${contract.devis.insurance.homeValue}</p>
                        <p><strong>Location:</strong> ${contract.devis.insurance.location}</p>
                    </c:if>
                    <p><strong>Subscription Date:</strong> ${contract.subscriptionDate}</p>
                    <p><strong>Expiration Date:</strong> ${contract.expirationDate}</p>
                    <p><strong>Document:</strong> <a href="${contract.document}">View Document</a></p>
                    <p><strong>Status:</strong>
                        <c:choose>
                            <c:when test="${contract.active}">Active</c:when>
                            <c:otherwise>Inactive</c:otherwise>
                        </c:choose>
                    </p>
                </div>
            </c:forEach>
        </div>
    </section>
